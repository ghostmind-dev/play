{
  "files.autoSave": "off",
  "editor.formatOnSave": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "prettier.singleQuote": true,
  "files.exclude": {
    "**/.gitignore": false,
    "**/.git": false,
    "**/.terraform": false,
    "**/.tfstate": false,
    "**/.npmrc": false,
    "**/dev/dvc-command": false,
    "**/.env.backup": false,
    "**/.dockerignore": false,
    "**/dvc-command": false,
    "**/gha**": false,
    "**/package-lock.json": false,
    "**/node_modules": false,
    "**/__pycache__": false,
    "**/.next": false,
    "**/.terraform.lock.hcl": false,
    "**/backend.tf": false,
    "**/versions.tf": false,
    "**/.npmignore": false,
    "**/.env": false
  },
  "files.associations": {
    "*.json": "jsonc"
  },
  "deno.enable": true,
  "deno.lint": true,
  "deno.cacheOnSave": true,
  "deno.path": "/deno/bin/deno",
  "deno.cache": "/deno/.cache",
  "deno.config": "/home/vscode/deno.json",
  "deno.disablePaths": [
    "/workspaces/play/actions/gcloud/app/src",
    "/workspaces/play/actions/run/app/src",
    "/workspaces/play/actions/vault/app/src",
    "/workspaces/play/actions/terraform/app/src",
    "/workspaces/play/actions/hasura/app/src",
    "/workspaces/play/actions/secrets/app/src",
    "/workspaces/play/actions/env/app/src"
  ],
  "vsicons.associations.folders": [
    { "icon": "app", "extensions": ["play"], "format": "svg" },
    { "icon": "log", "extensions": ["scripts"], "format": "svg" },
    { "icon": "audio", "extensions": ["dev"], "format": "svg" },
    { "icon": "bloc", "extensions": ["actions"], "format": "svg" },
    { "icon": "docker", "extensions": ["container"], "format": "svg" },
    { "icon": "e2e", "extensions": ["workflows"], "format": "svg" },
    { "icon": "cubit", "extensions": ["library-scripts"], "format": "svg" },
    { "icon": "buildkite", "extensions": ["gcloud"], "format": "svg" },
    { "icon": "buildkite", "extensions": ["hasura"], "format": "svg" },
    { "icon": "buildkite", "extensions": ["run"], "format": "svg" },
    { "icon": "buildkite", "extensions": ["terraform"], "format": "svg" },
    { "icon": "buildkite", "extensions": ["vault"], "format": "svg" },
    { "icon": "buildkite", "extensions": ["secrets"], "format": "svg" },
    { "icon": "buildkite", "extensions": ["env"], "format": "svg" }
  ],
  "vsicons.associations.files": [
    { "icon": "config", "extensions": [".env.local"], "format": "svg" },
    { "icon": "config", "extensions": [".env.dev"], "format": "svg" },
    { "icon": "config", "extensions": [".env.prod"], "format": "svg" },
    { "icon": "config", "extensions": [".env.template"], "format": "svg" },
    { "icon": "pytyped", "extensions": [".toml"], "format": "svg" },
    { "icon": "map", "extensions": [".md"], "format": "svg" }
  ],
  "python.analysis.diagnosticSeverityOverrides": {
    "reportMissingImports": "none",
    "reportMissingModuleSource": "none"
  },
  "json.schemas": [
    {
      "fileMatch": ["**/meta.json"],
      "url": "https://raw.githubusercontent.com/ghostmind-dev/config/main/config/meta/schema.json"
    }
  ]
}

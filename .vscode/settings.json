{
  "files.exclude": {
    "**/.gitignore": true,
    "**/.git": true,
    "**/.terraform": true,
    "**/.tfstate": true,
    "**/.npmrc": true,
    "**/dev/dvc-command": true,
    "**/.env.backup": true,
    "**/.dockerignore": true,
    "**/dvc-command": true,
    "**/gha**": true,
    "**/package-lock.json": true,
    "**/node_modules": true,
    "**/__pycache__": true,
    "**/.next": true,
    "**/dist": true
  },
  "files.associations": {
    "*.json": "jsonc"
  },
  "files.autoSave": "off",
  "editor.formatOnSave": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "prettier.singleQuote": true,
  "deno.unstable": true,
  "deno.lint": true,
  "deno.cacheOnSave": true,
  "deno.path": "/deno/bin/deno",
  "deno.cache": "/deno/.cache",
  "deno.config": "/home/vscode/deno.json",
  "deno.disablePaths": ["src"],
  "deno.enable": true
}
